# Echelon Contracts Deployment Configuration
# Copy to .env and fill in your values

# Deployer private key (DO NOT COMMIT!)
PRIVATE_KEY=

# Sepolia RPC URL (Alchemy or Infura)
SEPOLIA_RPC_URL=

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=

# ============================================
# Fund Manager Registration (run after deploy)
# ============================================

# Registry contract address (from deployment output)
REGISTRY_ADDRESS=0xCCf3E485bc5339C651f4fbb8F3c37881c0D0e704

# Fund Manager wallet address
# This is the wallet that will control the Fund Manager agent
# Can be the same as deployer wallet or a dedicated agent wallet
FUND_MANAGER_WALLET=
